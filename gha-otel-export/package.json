{
	"name": "gha-otel-exporter",
	"description": "GitHub Actions Opentelemetry Exporter",
	"version": "v0.0.1",
	"author": "",
	"private": true,
	"type": "module",
	"engines": {
		"node": ">=20"
	},
	"main": "dist/main.js",
	"scripts": {
		"bundle": "npm run format:write && npm run package",
		"format:write": "npx prettier --write .",
		"format:check": "npx prettier --check .",
		"package": "npx ncc build src/index.ts -o dist --source-map",
		"test": "jest --coverage",
		"all": "npm run format:write && npm run test && npm run package",
		"start": "tsx src/main.ts",
		"parse-local": "tsx src/parse-local.ts",
		"test-artifact": "tsx src/test-artifact-download.ts"
	},
	"dependencies": {
		"@actions/core": "^1.11.1",
		"@actions/github": "^6.0.0",
		"@octokit/rest": "^22.0.1",
		"@opentelemetry/api": "^1.9.0",
		"@opentelemetry/exporter-trace-otlp-proto": "^0.208.0",
		"@opentelemetry/resources": "^2.2.0",
		"@opentelemetry/sdk-trace-base": "^2.2.0",
		"adm-zip": "^0.5.16",
		"dotenv": "^16.3.1",
		"minimatch": "^9.0.5",
		"ts-retry": "^5.0.1"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.3.1",
		"@eslint/js": "^9.39.1",
		"@octokit/types": "^16.0.0",
		"@opentelemetry/sdk-node": "0.208.0",
		"@opentelemetry/semantic-conventions": "^1.38.0",
		"@types/adm-zip": "^0.5.7",
		"@types/jest": "^27.4.0",
		"@types/node": "^24.10.0",
		"@typescript-eslint/eslint-plugin": "^8.46.3",
		"@typescript-eslint/parser": "^8.46.0",
		"@vercel/ncc": "^0.38.4",
		"eslint": "^9.39.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-import-resolver-typescript": "^4.4.4",
		"eslint-plugin-github": "^5.1.7",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-jsonc": "^2.21.0",
		"eslint-plugin-prettier": "^5.5.4",
		"globals": "^16.5.0",
		"jest": "^27.4",
		"make-coverage-badge": "^1.2.0",
		"prettier": "^3.6.2",
		"prettier-eslint": "^16.4.2",
		"ts-jest": "^27.1.3",
		"ts-node": "^10.9.2",
		"tsx": "^4.20.6",
		"typescript": "~5.9.3"
	}
}
